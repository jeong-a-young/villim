<AnchorPane prefHeight="900.0" prefWidth="1600.0" stylesheets="@../application/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Information_Controller">
   <children>
      <ImageView fitHeight="970.0" fitWidth="1600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/informationPage.png" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="40.0" layoutY="34.0" mnemonicParsing="false" onAction="#back" prefHeight="50.0" prefWidth="162.0" styleClass="transparencyBtn">
