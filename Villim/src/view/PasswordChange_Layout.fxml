<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" stylesheets="@../application/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Profile_Controller">
   <children>
      <ImageView fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/passwordChangePage.png" />
         </image>
      </ImageView>
      <PasswordField fx:id="password" layoutX="53.0" layoutY="162.0" prefHeight="48.0" prefWidth="395.0">
         <font>
            <Font size="25.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="passwordCheck" layoutX="53.0" layoutY="267.0" prefHeight="48.0" prefWidth="395.0">
         <font>
            <Font size="25.0" />
         </font>
      </PasswordField>
      <Button layoutX="379.0" layoutY="346.0" mnemonicParsing="false" onAction="#passwordChange" prefHeight="33.0" prefWidth="104.0" styleClass="transparencyBtn">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
